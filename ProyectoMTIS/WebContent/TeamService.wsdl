<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.example.org/TeamService/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="TeamService" targetNamespace="http://www.example.org/TeamService/">
  <wsdl:types>
    <xsd:schema targetNamespace="http://www.example.org/TeamService/">
      <xsd:element name="createTeam">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="team" type="tns:Team"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
